<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="wwwroot\favicon.ico" />
    <Content Remove="wwwroot\imagess\bgimage.png" />
    <Content Remove="wwwroot\images\Farm central.png" />
    <Content Remove="wwwroot\pics\bgimage.png" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="image\bgimage.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="image\bgimage.png">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="5.0.17" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="wwwroot\favicon.ico" />
    <Resource Include="wwwroot\imagess\bgimage.png" />
    <Resource Include="wwwroot\images\Farm central.png" />
    <Resource Include="wwwroot\pics\bgimage.png" />
  </ItemGroup>

</Project>
